    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-vue": "1.1.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.3.0",
    "eslint-define-config": "^1.2.0",
    "eslint-plugin-vue": "^8.1.1",
    "postcss-pxtorem": "^6.0.0",
    "typescript": "^4.4.3",
